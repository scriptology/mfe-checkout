{
  "name": "mfe-checkout",
  "version": "4.4.3",
  "engines": {
    "node": ">=18.x"
  },
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "eslint ./ --ext .js,.jsx,.tsx,.ts",
    "lint:fix": "eslint ./ --ext .tsx,.ts --fix",
    "test": "playwright test",
    "test:debug": "PWDEBUG=1 playwright test",
    "serve": "serve -l tcp://localhost:$PORT/ -n -s out/dist",
    "dep:upgrade:major": "pnpm exec npm-check-updates -u",
    "dep:upgrade:minor": "pnpm dep:upgrade:major -t minor"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commercelayer/mfe-checkout.git"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      [
        "@semantic-release/github",
        {
          "addReleases": "top"
        }
      ]
    ]
  },
  "author": {
    "name": "Matteo Alessani",
    "email": "matteo@commercelayer.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commercelayer/mfe-checkout/issues"
  },
  "homepage": "https://github.com/commercelayer/mfe-checkout#readme",
  "keywords": [
    "reactjs",
    "nextjs",
    "jamstack",
    "headless",
    "ecommerce",
    "api",
    "checkout",
    "commercelayer"
  ],
  "dependencies": {
    "@commercelayer/react-components": "^4.8.6-beta.1",
    "@commercelayer/sdk": "^5.26.0",
    "@faker-js/faker": "^8.3.1",
    "@headlessui/react": "^1.7.17",
    "@next/bundle-analyzer": "^14.0.4",
    "@next/eslint-plugin-next": "^14.0.4",
    "@playwright/test": "^1.40.1",
    "@tailwindcss/forms": "^0.5.7",
    "@types/async-retry": "1.4.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-gtm-module": "^2.0.3",
    "@types/styled-components": "^5.1.34",
    "async-retry": "^1.3.3",
    "autoprefixer": "^10.4.16",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "classnames": "^2.5.1",
    "cron-parser": "^4.9.0",
    "cronstrue": "^2.47.0",
    "dotenv": "^16.3.1",
    "i18next": "^23.7.15",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "next": "^14.0.4",
    "next-build-id": "^3.0.0",
    "next-i18next": "^15.1.2",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-i18next": "^14.0.0",
    "react-router-dom": "^6.21.1",
    "styled-components": "^6.1.6",
    "tailwindcss": "^3.4.0",
    "twin.macro": "^3.4.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-tailwind": "^0.2.1",
    "npm-check-updates": "^16.14.12",
    "prettier": "^3.1.1",
    "semantic-release": "^22.0.12",
    "serve": "^14.2.1",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0"
  },
  "resolutions": {
    "npm": ">=8.18.0",
    "dset": ">=3.1.2",
    "semver": ">=7.5.2",
    "word-break": ">=1.2.5",
    "zod": ">=3.22.4"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "cacheDirectories": [
    ".next/cache"
  ]
}
